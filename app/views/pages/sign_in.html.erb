<div id="SignIn" ng-controller='SignInController'>
    <div style='text-align: center; margin-top: 40px;'>Pleaes place the correct<br>username and password to log in.</div>
    <form class='signin_container' ng-submit='signIn()'>
        <div class='signin_field_container'>
            <div class='signin_title'>Username</div>
            <input class='signin_field username' type='text' ng-model="username">
        </div>
        <div class='signin_field_container'>
            <div class='signin_title'>Password</div>
            <input class='signin_field password' type='password' ng-model="password">
        </div>
        <button type='submit' class='sign_in_button'>Sign In</button>
    </form>
</div>