<div id="Blog" ng-controller='BlogController'>
    <div class='post_container' ng-repeat="post in posts">
        <div class='post_title'>{{post.title}}</div>
        <div clas='post_date'>{{post.created_at.split("T")[0]}}</div>
        <div class='post_content'>{{post.content}}</div>
    </div>
</div>